forge "https://forgeapi.puppetlabs.com"

<% hash('/config/puppet/master')['modules'].each do |mod| -%>
mod '<%= mod['name'] %>'<%= ", '#{mod['version']}'" if mod['version'] %><%= ", :git => '#{mod['git']}'" if mod['git'] %><%= ", :github_tarball => '#{mod['github_tarball']}'" if mod['github_tarball'] %>
<% end -%>
